function makeMarkerArray(dataElementRoot,markerElementRoot,icon) {
		for(var index=0; index<dataElementRoot.length;index++){
			var data = dataElementRoot[index];
			var backup = false;

			if (data.title.indexOf('PHIL') > -1) {
				backup = icon;
				icon = '/map/images/website/darkgreen_Marker.png';
			} else if (false !== backup) {
				icon = backup;
				backup = false;
			}
			
			//console.dir("outside " + window.tempDataDescription);
			var marker = new google.maps.Marker({
				visible: false,
				position: new google.maps.LatLng(data.lat, data.lng),
				map: map,
				icon: icon,
				title: data.title,
				dist: data.dist,
				html: data.description
			});
			
			google.maps.event.addListener(marker, 'click', function(){
				infoWindow.setContent(this.html);
				infoWindow.open(map, this);
				/*setTimeout(function (){ 
					infoWindow.close(); 
				}, 6000); */
			});
			
			markerElementRoot.push(marker);
			//data = null;
		}	
	}

	makeMarkerArray(all_markers.studentparking,markers.studentparking,'/map/images/website/darkgreen_Marker.png');

